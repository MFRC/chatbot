<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CustomerService.
    -->
    <changeSet id="20230622151215-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="faqs_id"
                                 baseTableName="customer_service"
                                 constraintName="fk_customer_service__faqs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fa_qs"/>

        <addForeignKeyConstraint baseColumnNames="customer_service_entity_id"
                                 baseTableName="customer_service"
                                 constraintName="fk_customer_service__customer_service_entity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer_service_entity"/>

        <addForeignKeyConstraint baseColumnNames="customer_service_user_id"
                                 baseTableName="customer_service"
                                 constraintName="fk_customer_service__customer_service_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer_service_user"/>
    </changeSet>
</databaseChangeLog>
